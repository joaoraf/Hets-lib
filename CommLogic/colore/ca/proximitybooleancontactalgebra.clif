(cl-module (ProximityBooleanContactAlgebra)

(cl-comment 'Proximity Boolean Contact algebra (PBCA) axioms')

(cl-import (BooleanContactAlgebra))


(cl-comment 'Nor: Normality axiom ')

(forall (x y) 	
	(if 	(not (C x y)) 
		(exists (u v)
			(and 	(not (C x u))
				(not (C y v))
				(= (join u v) 1)))))

)
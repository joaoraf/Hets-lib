(cl-module (RegionBooleanContactAlgebra)

(cl-comment 'Region Boolean Contact algebra (RBCA) axioms')

(cl-import (BooleanContactAlgebra))


(cl-comment 'Con: Connection axiom (connection of an entity with its complement)')

(forall (x) 	
	(if 	(and 	(not (= x 0)) 
			(not (= x 1))) 
		(exists (y)
			(and 	(complement x y)
				(C x y)))))

)

(cl-module weak-tripartite.clif

(forall (x)
	(if	(point x)
		(not (line x))))

(forall (x)
	(if	(point x)
		(not (plane x))))

(forall (x)
	(if	(line x)
		(not (plane x))))

(forall (x y)
	(if	(in x y)
		(or	(and	(point x)
				(line y))
			(and	(point x)
				(plane y))
			(and	(line x)
				(plane y)))))

(forall (y)
	(if	(line y)
		(exists (x)
			(in x y))))

(forall (x)
	(if	(line x)
		(exists (y)
			(in x y))))

)

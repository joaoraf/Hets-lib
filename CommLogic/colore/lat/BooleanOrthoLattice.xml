<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../COLORE.xsl"?>

<cl-module 	xmlns:cl="http://stl.mie.utoronto.ca/ColoreSchema" 
					xmlns:lat="http://stl.mie.utoronto.ca/Colore/Lattices" 
					xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
					xmlns="http://stl.mie.utoronto.ca/ColoreSchema"
					targetNamespace="http://stl.mie.utoronto.ca/Colore/Lattices">

	<moduleDefinition module="BooleanOrthoLattice" axiomFile="ortholattice.clif" />

	<head>
		<version>1.0</version>
		<date>2010-02-19</date>
		<description>Orthocomplemented lattices</description>
		<keyword>OrthocomplementedLattice</keyword>
		<keyword>Boolean</keyword>
		<keyword>Ortholattice</keyword>
		<author>Torsten Hahmann</author>
		<parentOntology>
			<name>Lattices</name>
		</parentOntology>
		<source>
			<name>Axioms for Lattices and Boolean Algebras</name>
			<author>Padmanabhan</author>
			<author>Rudeanu</author>
			<year>2008</year>
		</source>
	</head>

	<lexicon/>

	<inheritedLexicon>
		<primitiveLexicalSymbol symbol="meet" type="function" arity="2"/>
		<primitiveLexicalSymbol symbol="join" type="function" arity="2"/>
		<primitiveLexicalSymbol symbol="oc" type="function" arity="2"/>
	</inheritedLexicon>

	<directRelations>
		<extends extensionType="non-conservative">lat:OrthoLattice</extends>
		<definablyEquivalent>lat:BooleanLattice</definablyEquivalent>
		<definablyEquivalent>lat:BooleanPOrthoLattice</definablyEquivalent>
	</directRelations>

	<inheritedRelations>
		<relativelyInterprets extensionType="non-conservative">lat:Lattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:LatticeMeetJoin</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:BoundedLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:ModularLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:DistributiveLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:BoundedDistributiveLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:ComplementedLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:PseudocomplementedLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:OrthocomplementedLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:OrthoModularLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:StonianPOrthoLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:UnicomplementedLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:LatticePoset</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">pos:Poset</relativelyInterprets>
	</inheritedRelations>

	<additionalInformation/>

</cl-module>
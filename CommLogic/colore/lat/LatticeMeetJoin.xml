<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../COLORE.xsl"?>

<cl-module xmlns:cl="http://stl.mie.utoronto.ca/ColoreSchema" 
					xmlns:lat="http://stl.mie.utoronto.ca/Colore/Lattices" 
					xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
					xmlns="http://stl.mie.utoronto.ca/ColoreSchema"
					targetNamespace="http://stl.mie.utoronto.ca/Colore/Lattices">

	<moduleDefinition module="LatticeMeetJoin" axiomFile="lattice_meetjoin.clif" />

	<head>
		<version>1.1</version>
		<date>2009-07-06</date>
		<description>Axioms for arbitrary lattices based on meet and join relations</description>
		<keyword>Lattice</keyword>
		<keyword>Meet</keyword>
		<keyword>Join</keyword>
		<author>Torsten Hahmann</author>
		<parentOntology>
			<name>Lattices</name>
		</parentOntology>
		<source>
			<name>Axioms for Lattices and Boolean Algebras</name>
			<author>Padmanabhan</author>
			<author>Rudeanu</author>
			<year>2008</year>
		</source>
	</head>

	<lexicon/>

	<inheritedLexicon>
		<primitiveLexicalSymbol symbol="meet" type="function" arity="2"/>
		<primitiveLexicalSymbol symbol="join" type="function" arity="2"/>
	</inheritedLexicon>

	<directRelations>
		<extendedBy extensionType="non-conservative">lat:BoundedLattice</extendedBy>
		<extendedBy extensionType="non-conservative">lat:DistributiveLattice</extendedBy>
		<definablyEquivalent mappingExtension="lat:Lattice">lat:LatticePoset</definablyEquivalent>
	</directRelations>

	<inheritedRelations/>

	<additionalInformation/>

</cl-module>
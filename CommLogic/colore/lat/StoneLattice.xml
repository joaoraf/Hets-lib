<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../COLORE.xsl"?>

<cl-module 	xmlns:cl="http://stl.mie.utoronto.ca/ColoreSchema" 
					xmlns:lat="http://stl.mie.utoronto.ca/Colore/Lattices" 
					xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
					xmlns="http://stl.mie.utoronto.ca/ColoreSchema"
					targetNamespace="http://stl.mie.utoronto.ca/Colore/Lattices">

	<moduleDefinition module="StoneLattice" axiomFile="stonelattice.clif" />

	<head>
		<version>1.0</version>
		<date>2010-02-26</date>
		<description>Pseudocomplemented distributive lattices satisfying the Stone identity</description>
		<keyword>Stone Lattice</keyword>
		<keyword>PseudocomplementedDistributiveLattice</keyword>
		<keyword>Pseudocomplement</keyword>
		<keyword>Distributivity</keyword>
		<author>Torsten Hahmann</author>
		<parentOntology>
			<name>Lattices</name>
		</parentOntology>
	</head>

	<lexicon/>

	<inheritedLexicon>
		<primitiveLexicalSymbol symbol="0" type="constant" arity="0"/>
		<primitiveLexicalSymbol symbol="1" type="constant" arity="0"/>
		<primitiveLexicalSymbol symbol="meet" type="function" arity="2"/>
		<primitiveLexicalSymbol symbol="join" type="function" arity="2"/>
		<primitiveLexicalSymbol symbol="pc" type="function" arity="2"/>
		<definedLexicalSymbol symbol="leq" type="relation" arity="2"/>
	</inheritedLexicon>

	<directRelations>
		<extends extensionType="non-conservative">lat:PseudocomplementedDistributiveLattice</extends>
	</directRelations>

	<inheritedRelations>
		<extends extensionType="non-conservative">lat:PseudocomplementedLattice</extends>
		<extends extensionType="non-conservative">lat:DistributiveLattice</extends>
		<extends extensionType="non-conservative">lat:BoundedLattice</extends>
		<extends extensionType="non-conservative">lat:Lattice</extends>
		<extends extensionType="non-conservative">lat:LatticeMeetJoin</extends>
		<relativelyInterprets extensionType="non-conservative">lat:LatticePoset</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">pos:Poset</relativelyInterprets>
	</inheritedRelations>

	<additionalInformation/>

</cl-module>
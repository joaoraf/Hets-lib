(cl-module StoneLattice

(cl-imports PseudocomplementedDistributiveLattice)

(cl-comment 'Defining the Stone lattices as pseudocomplemented distributive lattices satisfying the Stone identity')

(cl-comment 'S1: Stone identity')

(forall (x y)
	(= (pc (meet x y)) (join (pc x) (pc y)))
)


)

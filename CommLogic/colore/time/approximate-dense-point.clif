
(cl-module approximate-dense-point.clif

(cl-imports approximate-point.clif)

(forall (x y)
(iff	(meets x y)
	(and	(precedes x y)
		(forall (z)
			(if	(and	(ncdf x z)
					(ncdf z y))
				(exists (u)
					(and	(finer u z)
						(ncdf x u)
						(ncdf u y))))))))

(forall (x)
	(exists (y)
		(and	(finer y x)
			(not (finer  x y)))))

)



(cl-module approximate-discrete-point.clif

(cl-imports approximate-point.clif)

(forall (x y)
(iff	(meets x y)
	(and	(precedes x y)
		(not (exists (z)
			(and	(precedes x z)
				(precedes z y)))))))

(forall (x)
(iff	(moment x)
	(not (exists (y)
		(and	(finer y x)
			(not (=  x y)))))))

(forall (x)
	(exists (y)
		(and	(meets x y)
			(moment y))))

(forall (x)
	(exists (y)
		(and	(meets y x)
			(moment y))))

)


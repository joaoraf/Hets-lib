(cl:module Partial_Order
  (cl:comment 'reflexive')
  (forall (x)
          (le x x))
  (cl:comment 'asymmetric')
  (forall (x y)
          (if (and (le x y)
                   (le y x))
              (= x y)))
  (cl:comment 'transitive')
  (forall (x y z)
          (if (and (le x y)
                   (le y z))
              (le x z))))
<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../COLORE.xsl"?>

<cl-module 	xmlns:cl="http://stl.mie.utoronto.ca/ColoreSchema" 
					xmlns:lat="http://stl.mie.utoronto.ca/Colore/Lattices" 
					xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
					xmlns="http://stl.mie.utoronto.ca/ColoreSchema"
					targetNamespace="http://stl.mie.utoronto.ca/Colore/Lattices">

	<moduleDefinition module="BooleanLattice" axiomFile="booleanlattice.clif" />

	<head>
		<version>1.0</version>
		<date>2009-07-06</date>
		<description>Boolean lattice as complemented bounded distributive lattices</description>
		<keyword>Boolean Lattice</keyword>
		<keyword>Complemented</keyword>
		<keyword>Distributive</keyword>
		<author>Torsten Hahmann</author>
		<parentOntology>
			<name>Lattices</name>
		</parentOntology>
	</head>

	<lexicon/>

	<inheritedLexicon>
		<primitiveLexicalSymbol symbol="0" type="constant" arity="0"/>
		<primitiveLexicalSymbol symbol="1" type="constant" arity="0"/>
		<primitiveLexicalSymbol symbol="meet" type="function" arity="2"/>
		<primitiveLexicalSymbol symbol="join" type="function" arity="2"/>
		<definedLexicalSymbol symbol="leq" type="relation" arity="2"/>
		<definedLexicalSymbol symbol="meetcomplement" type="relation" arity="2"/>
		<definedLexicalSymbol symbol="joincomplement" type="relation" arity="2"/>
		<definedLexicalSymbol symbol="complement" type="relation" arity="2"/>
	</inheritedLexicon>

	<directRelations>
		<extends extensionType="non-conservative">lat:BoundedDistributiveLattice</extends>
		<extends extensionType="non-conservative">lat:ComplementedLattice</extends>
		<definablyEquivalent>lat:BooleanOrthoLattice</definablyEquivalent>
		<definablyEquivalent>lat:BooleanPOrthoLattice</definablyEquivalent>
	</directRelations>

	<inheritedRelations>
		<extends extensionType="non-conservative">lat:Lattice</extends>
		<extends extensionType="non-conservative">lat:LatticeMeetJoin</extends>
		<extends extensionType="non-conservative">lat:BoundedLattice</extends>
		<extends extensionType="non-conservative">lat:ModularLattice</extends>
		<extends extensionType="non-conservative">lat:DistributiveLattice</extends>
		<extends extensionType="non-conservative">lat:UnicomplementedLattice</extends>
		<relativelyInterprets extensionType="non-conservative">pos:Poset</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:PatticePoset</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:PseudocomplementedLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:QuasicomplementedLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:OrthocomplementedLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:OrthoLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:OrthoModularLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:POrthoLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:StonianPOrthoLattice</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:StonianPOrthoLatticeMeet</relativelyInterprets>
	</inheritedRelations>

	<additionalInformation/>

</cl-module>
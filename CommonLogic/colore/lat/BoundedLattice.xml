<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../COLORE.xsl"?>

<cl-module 	xmlns:cl="http://stl.mie.utoronto.ca/ColoreSchema" 
					xmlns:lat="http://stl.mie.utoronto.ca/Colore/Lattices" 
					xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
					xmlns="http://stl.mie.utoronto.ca/ColoreSchema"
					targetNamespace="http://stl.mie.utoronto.ca/Colore/Lattices">

	<moduleDefinition module="boundedLattice" axiomFile="boundedlattice.clif" />

	<head>
		<version>1.2</version>
		<date>2010-02-19</date>
		<description>Bounded lattices based on the lattice axioms defined in terms of meet and join</description>
		<keyword>BoundedLattice</keyword>
		<author>Torsten Hahmann</author>
		<parentOntology>
			<name>Lattices</name>
		</parentOntology>
		<source>
			<name>Axioms for Lattices and Boolean Algebras</name>
			<author>Padmanabhan</author>
			<author>Rudeanu</author>
			<year>2008</year>
		</source>
	</head>

	<lexicon>
		<primitiveLexicalSymbol symbol="zero" type="constant" arity="0"/>
		<primitiveLexicalSymbol symbol="one" type="constant" arity="0"/>
	</lexicon>

	<inheritedLexicon>
		<primitiveLexicalSymbol symbol="meet" type="function" arity="2"/>
		<primitiveLexicalSymbol symbol="join" type="function" arity="2"/>
		<definedLexicalSymbol symbol="leq" type="function" arity="2"/>
	</inheritedLexicon>

	<directRelations>
		<extends extensionType="non-conservative">lat:Lattice</extends>
	</directRelations>

	<inheritedRelations>
		<extends extensionType="non-conservative">lat:LatticeMeetJoin</extends>
		<relativelyInterprets extensionType="non-conservative">pos:Poset</relativelyInterprets>
		<relativelyInterprets extensionType="non-conservative">lat:LatticePoset</relativelyInterprets>
	</inheritedRelations>

	<additionalInformation/>

</cl-module>

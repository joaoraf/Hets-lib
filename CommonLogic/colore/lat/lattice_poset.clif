(cl-module Lattice_Poset

(cl-imports Poset)

(cl-comment 'Universal lattice axioms')

(cl-comment 'P4, D1: Unique meet')

(forall (x y) 
	(exists (m)
		(and 	(leq m x)
			(leq m y)
			(forall (z)
				(if	(and	(leq z x)
						(leq z y))
					(leq z m))))))
						
(cl-comment 'P5, D2: Unique join')

(forall (x y) 
	(exists (j)
		(and 	(leq x j)
			(leq y j)
			(forall (z)
				(if	(and	(leq x z)
						(leq y z))
					(leq j z))))))

)

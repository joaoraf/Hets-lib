<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../COLORE.xsl"?>

<cl-module 	xmlns:cl="http://stl.mie.utoronto.ca/ColoreSchema" 
					xmlns:mt="http://stl.mie.utoronto.ca/Colore/Mereotopologies" 
					xmlns:lat="http://stl.mie.utoronto.ca/Colore/Lattices" 
					xmlns:ca="http://stl.mie.utoronto.ca/Colore/ContactAlgebras"
					xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
					xmlns="http://stl.mie.utoronto.ca/ColoreSchema"
					targetNamespace="http://stl.mie.utoronto.ca/Colore/Mereotopologies">

	<moduleDefinition module="RCC-RBCA" axiomFile="rcc-rbca.clif" />

	<head>
		<version>1.0</version>
		<date>2010-03-05</date>
		<description>Mapping axioms between the Region Connection Calculus (RCC) and (Region) Boolean Connection Algebras (RBCA)</description>
		<keyword>RCC</keyword>
		<keyword>RBCA</keyword>
		<keyword>BCA</keyword>
		<keyword>Region</keyword>
		<keyword>Mereotopology</keyword>
		<keyword>Atomless Boolean Algebra</keyword>
		<author>Torsten Hahmann</author>
		<parentOntology>
			<name>Mereotopologies</name>
		</parentOntology>
	</head>

	<lexicon>
		<primitiveLexicalSymbol symbol="0" type="constant" arity="0"/>
		<primitiveLexicalSymbol symbol="meet" type="function" arity="2"/>
		<definedLexicalSymbol symbol="leq" type="relation" arity="2"/>		
	</lexicon>

	<inheritedLexicon>
		<primitiveLexicalSymbol symbol="C" type="relation" arity="2"/>		
		<definedLexicalSymbol symbol="-" type="function" arity="1"/>
		<definedLexicalSymbol symbol="sum" type="function" arity="2"/>
		<definedLexicalSymbol symbol="prod" type="function" arity="2"/>
		<definedLexicalSymbol symbol="P" type="relation" arity="2"/>		
		<definedLexicalSymbol symbol="PP" type="relation" arity="2"/>		
		<definedLexicalSymbol symbol="O" type="relation" arity="2"/>		
		<definedLexicalSymbol symbol="NTPP" type="relation" arity="2"/>		
	</inheritedLexicon>

	<directRelations>
		<extends extensionType="definitional">mt:RCC</extends>
		<maps extensionType="definitional">mt:RCC</maps>
		<maps extensionType="conservative">ca:RegionBooleanContactAlgebra</maps>
		<maps extensionType="conservative">ca:AtomlessBooleanContactAlgebra</maps>
		<faithfullyInterprets extensionType="non-conservative">lat:AtomlessBooleanLattice</faithfullyInterprets>
		<relativelyInterprets extensionType="non-conservative">con:ExtensionalContact</relativelyInterprets>
	</directRelations>

	<inheritedRelations/>

	<additionalInformation/>

</cl-module>
library Modal/TemporalLogicConcurrency

logic Modal

spec Temporal_Logic_Concurrency =
	modalities [empty],X,U {
				[](A => B)=>([]A=>[]B);
				X(A => B)=>(X A => X B);
				X not A <=> not X A;
				[]A => A /\ X [] A;
				[](A=>X A)=>(A=> [] A));
				A U B => <> A;
				A U B => B \/ (A /\ X (A U B))
				}
end
